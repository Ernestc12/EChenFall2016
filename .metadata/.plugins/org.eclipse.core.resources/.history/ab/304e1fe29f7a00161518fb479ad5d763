
public class DoMath {

	public static void main(String[] args) {
		Calculate.square(5);
		System.out.println(Calculate.square(5));
		Calculate.cube(3);
		System.out.println(Calculate.cube(3));
		Calculate.average(4, 100);
		System.out.println(Calculate.average(4, 100));
		Calculate.average(100, 50, 30);
		System.out.println(Calculate.average(100, 50, 30));
		Calculate.toImproperFrac(3, 1, 2);
		System.out.println(Calculate.toImproperFrac(3, 1, 2));
		Calculate.toMixedNum(7, 2);
		System.out.println(Calculate.toMixedNum(7, 2));
		Calculate.foil(2, 3, 6, 7, "n");
		System.out.println(Calculate.foil(2, 3, 6, -7, "n"));
		Calculate.absValue(-11);
		System.out.println(Calculate.absValue(-11));
		Calculate.exponent(7,3);
		System.out.println(Calculate.exponent(7, 3));
		Calculate.factorial(4);
		System.out.println(Calculate.factorial(5));
		// TODO Auto-generated method stub

	}
	
	

}
